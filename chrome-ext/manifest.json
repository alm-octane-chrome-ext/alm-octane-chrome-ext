{
  "name": "Octanetopus",
  "short_name": "Octanetopus",
  "description": "Unofficial extension for ALM Octane",
  "version": "1.0.17",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48": "img/icon48.png",
    "96": "img/icon96.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "common/octanetopus-common.js",
      "background/octanetopus-background.js"
    ]
  },
  "browser_action": {
    "default_title": "Octanetopus",
    "default_icon": "img/icon96.png",
    "default_popup": "popup/octanetopus-popup.html"
  },
  "content_scripts": [
  ],
  "web_accessible_resources": [
    "img/flags/*.svg",
    "img/note.svg",
    "img/radio.svg",
    "img/arrow-left.svg",
    "img/arrow-right.svg"
  ]
}
